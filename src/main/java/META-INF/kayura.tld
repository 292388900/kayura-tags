<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<tlib-version>1.0</tlib-version>
	<short-name>jeasyui-tags</short-name>
	<uri>http://www.kayura.org/tags/jeasyui</uri>

	<tag>
		<name>accordion</name>
		<tag-class>org.kayura.tags.easyui.AccordionTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>id</name>
			<description>菜单标签ID</description>
		</attribute>
		<attribute>
			<name>style</name>
			<description>HTML标签样式定义.</description>
		</attribute>
		<attribute>
			<name>width</name>
			<description>分类容器的宽度。</description>
		</attribute>
		<attribute>
			<name>height</name>
			<description>分类容器的高度。</description>
		</attribute>
		<attribute>
			<name>fit</name>
			<description>如果设置为true，分类容器大小将自适应父容器</description>
		</attribute>
		<attribute>
			<name>border</name>
			<description>定义是否显示边框。</description>
		</attribute>
		<attribute>
			<name>animate</name>
			<description>定义在展开和折叠的时候是否显示动画效果。</description>
		</attribute>
		<attribute>
			<name>multiple</name>
			<description>如果为true时，同时展开多个面板。</description>
		</attribute>
		<attribute>
			<name>selected</name>
			<description>设置初始化时默认选中的面板索引号。</description>
		</attribute>
		<attribute>
			<name>onSelect</name>
			<description>在面板被选中的时候触发。函数定义：function(title,index)。</description>
		</attribute>
		<attribute>
			<name>onUnselect</name>
			<description>在面板被取消选中的时候触发。函数定义：function(title,index)。</description>
		</attribute>
		<attribute>
			<name>onAdd</name>
			<description>在添加新面板的时候触发。函数定义：function(title,index)。</description>
		</attribute>
		<attribute>
			<name>onBeforeRemove</name>
			<description>title,index 在移除面板之前触发，返回false可以取消移除操作。函数定义：function(title,index)。</description>
		</attribute>
		<attribute>
			<name>onRemove</name>
			<description>在面板被移除的时候触发。函数定义：function(title,index)。</description>
		</attribute>
	</tag>
	
	<tag>
		<name>sheet</name>
		<tag-class>org.kayura.tags.easyui.SheetTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>id</name>
			<description>面板的标签ID.</description>
		</attribute>
		<attribute>
			<name>title</name>
			<description>在面板头部显示的标题文本。</description>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>iconCls</name>
			<description>设置一个16x16图标的CSS类ID显示在面板左上角。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>padding</name>
			<description>设置内间距。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>selected</name>
			<description>如果设置为true将展开面板。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>collapsible</name>
			<description>如果设置为true将显示折叠按钮。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>width</name>
			<description>设置面板宽度。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>height</name>
			<description>设置面板高度。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>left</name>
			<description>设置面板距离左边的位置（即X轴位置）。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>top</name>
			<description>设置面板距离顶部的位置（即Y轴位置）。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>cls</name>
			<description>添加一个CSS类ID到面板。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>headerCls</name>
			<description>添加一个CSS类ID到面板头部。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>bodyCls</name>
			<description>添加一个CSS类ID到面板正文部分。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>fit</name>
			<description>当设置为true的时候面板大小将自适应父容器。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>border</name>
			<description>定义是否显示面板边框。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>doSize</name>
			<description>如果设置为true，在面板被创建的时候将重置大小和重新布局。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>noheader</name>
			<description>如果设置为true，那么将不会创建面板标题。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>content</name>
			<description>面板主体内容。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>collapsible</name>
			<description>定义是否显示可折叠按钮。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>minimizable</name>
			<description>定义是否显示最小化按钮。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>maximizable</name>
			<description>定义是否显示最大化按钮。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>closable</name>
			<description>定义是否显示关闭按钮。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>tools</name>
			<description>自定义工具菜单，可用值：1) 数组，每个元素都包含'iconCls'和'handler'属性。2) 指向工具菜单的选择器。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>header</name>
			<description>定义面板标题。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>footer</name>
			<description>定义面板页脚。可用值：1) 数组，每个元素都包含'iconCls'和'handler'属性。2) 指向工具菜单的选择器。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>openAnimation</name>
			<description>定义打开面板的动画，可用值有：'slide','fade','show'。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>openDuration</name>
			<description>定义打开面板的持续时间。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>closeAnimation</name>
			<description>定义关闭面板的动画，可用值有：'slide','fade','show'。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>closeDuration</name>
			<description>定义关闭面板的持续时间。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>collapsed</name>
			<description>定义是否在初始化的时候折叠面板。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>minimized</name>
			<description>定义是否在初始化的时候最小化面板。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>maximized</name>
			<description>定义是否在初始化的时候最大化面板。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>closed</name>
			<description>定义是否在初始化的时候关闭面板。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>href</name>
			<description>从URL读取远程数据并且显示到面板。注意：内容将不会被载入，直到面板打开或扩大，在创建延迟加载面板时是非常有用的： </description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>cache</name>
			<description>如果为true，在超链接载入时缓存面板内容。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>loadingMessage</name>
			<description>在加载远程数据的时候在面板内显示一条消息。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>extractor</name>
			<description>定义如何从ajax应答数据中提取内容，返回提取数据。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>method</name>
			<description>使用HTTP的哪一种方法读取内容页。可用值：'get','post'。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>queryParams</name>
			<description>在加载内容页的时候添加的请求参数。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>loader</name>
			<description>
			定义了如何从远程服务器加载内容页，该函数接受以下参数：
			param：参数对象发送给远程服务器。
			success(data)：在检索数据成功的时候调用的回调函数。
			error()：在检索数据失败的时候调用的回调函数。
			</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<name>menu</name>
		<tag-class>org.kayura.tags.easyui.MenuTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>id</name>
			<description>菜单标签ID</description>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<description>Body 样式定义.</description>
		</attribute>
		<attribute>
			<name>menus</name>
			<description>搜索类型菜单数据。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>zIndex</name>
			<description>菜单z-index样式，增加它的值。</description>
		</attribute>
		<attribute>
			<name>left</name>
			<description>菜单的左边距位置。</description>
		</attribute>
		<attribute>
			<name>top</name>
			<description>菜单的上边距位置。</description>
		</attribute>
		<attribute>
			<name>align</name>
			<description>菜单对齐方式，可用值有：'left' 和 'right'。</description>
		</attribute>
		<attribute>
			<name>minWidth</name>
			<description>菜单的最小宽度。</description>
		</attribute>
		<attribute>
			<name>itemHeight</name>
			<description>菜单项高度。</description>
		</attribute>
		<attribute>
			<name>duration</name>
			<description>该属性允许用户自定义隐藏菜单动画的持续时间，以毫秒为单位。</description>
		</attribute>
		<attribute>
			<name>hideOnUnhover</name>
			<description>当设置为true时，在鼠标离开菜单的时候将自动隐藏菜单。</description>
		</attribute>
		<attribute>
			<name>inline</name>
			<description>当设置为true时，菜单将相对于父级标签进行定位，否则相对于body进行定位。</description>
		</attribute>
		<attribute>
			<name>fit</name>
			<description>当设置为true时，菜单尺寸将自动适配父容器。</description>
		</attribute>
		<attribute>
			<name>noline</name>
			<description>当设置为true时，菜单将不显示图标和文字之间的分割线。</description>
		</attribute>
		<attribute>
			<name>onShow</name>
			<description>在菜单显示之后触发。</description>
		</attribute>
		<attribute>
			<name>onHide</name>
			<description>在菜单隐藏之后触发。</description>
		</attribute>
		<attribute>
			<name>onClick</name>
			<description>在菜单项被点击的时候触发。function menuHandler(item)。</description>
		</attribute>
	</tag>

	<tag>
		<name>submenu</name>
		<tag-class>org.kayura.tags.easyui.SubMenuTag</tag-class>
		<attribute>
			<name>id</name>
			<description>菜单标签ID</description>
		</attribute>
		<attribute>
			<name>text</name>
			<description>菜单项文本。</description>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<description>Body 样式定义.</description>
		</attribute>
		<attribute>
			<name>iconCls</name>
			<description>显示在菜单项左侧的16x16像素图标的CSS类ID。</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<description>定义是否禁用菜单项。</description>
		</attribute>
		<attribute>
			<name>isCustom</name>
			<description>该子菜单内容是否显示为自定义内容。</description>
		</attribute>
	</tag>
	
	<tag>
		<name>menuitem</name>
		<tag-class>org.kayura.tags.easyui.MenuItemTag</tag-class>
		<attribute>
			<name>id</name>
			<description>菜单标签ID</description>
		</attribute>
		<attribute>
			<name>name</name>
			<description>菜单项 name，若需响应菜单事件，必需定义该值。</description>
		</attribute>
		<attribute>
			<name>text</name>
			<description>菜单项文本。</description>
		</attribute>
		<attribute>
			<name>iconCls</name>
			<description>显示在菜单项左侧的16x16像素图标的CSS类ID。</description>
		</attribute>
		<attribute>
			<name>href</name>
			<description>设置点击菜单项时候的页面位置。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>disabled</name>
			<description>定义是否禁用菜单项。</description>
		</attribute>
		<attribute>
			<name>isSeparator</name>
			<description>标注此菜单是否为间隔符。</description>
		</attribute>
		<attribute>
			<name>selected</name>
			<description>是否默认为选中的项。</description>
		</attribute>
		<attribute>
			<name>onClick</name>
			<description>在点击菜单项的时候调用的函数。</description>
		</attribute>
	</tag>

	<tag>
		<name>splitbutton</name>
		<tag-class>org.kayura.tags.easyui.SplitButtonTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>id</name>
			<description>菜单标签ID</description>
		</attribute>
		<attribute>
			<name>text</name>
			<description>按钮文字。</description>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<description>Body 样式定义.</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<description>为true时禁用按钮。</description>
		</attribute>
		<attribute>
			<name>toggle</name>
			<description>为true时允许用户切换其状态是被选中还是未选择，可实现checkbox复选效果。</description>
		</attribute>
		<attribute>
			<name>selected</name>
			<description>定义按钮初始的选择状态，true为被选中，false为未选中。</description>
		</attribute>
		<attribute>
			<name>group</name>
			<description>指定相同组名称的按钮同属于一个组，可实现radio单选效果。</description>
		</attribute>
		<attribute>
			<name>plain</name>
			<description>为true时显示简洁效果。</description>
		</attribute>
		<attribute>
			<name>iconCls</name>
			<description>显示在按钮文字左侧的图标(16x16)的CSS类ID。</description>
		</attribute>
		<attribute>
			<name>iconAlign</name>
			<description>按钮图标位置。可用值有：'left','right','top','bottom'。</description>
		</attribute>
		<attribute>
			<name>size</name>
			<description>按钮大小。可用值有：'small','large'。</description>
		</attribute>
		<attribute>
			<name>menu</name>
			<description>用来创建一个对应菜单的选择器。</description>
		</attribute>
		<attribute>
			<name>duration</name>
			<description>定义鼠标划过按钮时显示菜单所持续的时间，单位为毫秒。</description>
		</attribute>
		<attribute>
			<name>menus</name>
			<description>下拉菜单数据。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>onClick</name>
			<description>在点击按钮的时候触发。</description>
		</attribute>
	</tag>

