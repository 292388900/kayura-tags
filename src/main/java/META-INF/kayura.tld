		<name>menubutton</name>
		<tag-class>org.kayura.tags.easyui.MenuButtonTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>id</name>
			<description>菜单标签ID</description>
		</attribute>
		<attribute>
			<name>text</name>
			<description>按钮文字。</description>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<description>Body 样式定义.</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<description>为true时禁用按钮。</description>
		</attribute>
